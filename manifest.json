{
    "manifest_version": 2,
    "name": "ytDislikes",
    "description": "Show youtube dislikes",
    "version": "1.0",
    "icons": {
        "16": "./Assets/16.png",
        "48": "./Assets/48.png",
        "128": "./Assets/128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "./Assets/16.png",
            "48": "./Assets/48.png",
            "128": "./Assets/128.png"
        }
    },
    "content_scripts": [{
        "matches": ["https://www.youtube.com/watch?v=*"],
        "js": ["main.js"]
    }]
}